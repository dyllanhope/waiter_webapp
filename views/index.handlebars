<div class='row'>
    <div class='col-12 center'>
        <h1>Waiter login</h1>
    </div>
</div>
<div class='row'>
    <div class='col-12 center'>
        <ul>
        {{#each days}}
        <li class='{{style}}'><u>{{day}}</u> <br> waiters: {{waiters}}</li>
        {{/each}}
        </ul>
    </div>
</div>
<div class='row'>
    <div class='col-12 center'>
            {{#each waiters}}
                <a href="/waiters/{{waiter_name}}">{{waiter_name}}</a> <br>
            {{/each}}
    </div>
</div>
<div class='row'>
    <div class='col-12 center'>
        <form action="/clear" method="POST">
            <input type="submit" class="loginBtn" value="Clear">
        </form>
    </div>
</div>